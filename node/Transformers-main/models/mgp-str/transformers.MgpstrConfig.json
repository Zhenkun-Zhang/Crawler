{
    "api": "transformers.MgpstrConfig",
    "type": "class",
    "version": "main",
    "args_list": [
        "image_size",
        "128]",
        "patch_size",
        "num_channels",
        "max_token_length",
        "num_character_labels",
        "num_bpe_labels",
        "num_wordpiece_labels",
        "hidden_size",
        "num_hidden_layers",
        "num_attention_heads",
        "mlp_ratio",
        "qkv_bias",
        "distilled",
        "layer_norm_eps",
        "drop_rate",
        "attn_drop_rate",
        "drop_path_rate",
        "output_a3_attentions",
        "initializer_range",
        "**kwargs"
    ],
    "params": [
        {
            "name": "image_size",
            "type": "List[int]",
            "optional": true,
            "default": "[32,",
            "description": "The size (resolution) of each image."
        },
        {
            "name": "patch_size",
            "type": "int",
            "optional": true,
            "default": "4",
            "description": "The size (resolution) of each patch."
        },
        {
            "name": "num_channels",
            "type": "int",
            "optional": true,
            "default": "3",
            "description": "The number of input channels."
        },
        {
            "name": "max_token_length",
            "type": "int",
            "optional": true,
            "default": "27",
            "description": "The max number of output tokens."
        },
        {
            "name": "num_character_labels",
            "type": "int",
            "optional": true,
            "default": "38",
            "description": "The number of classes for character head ."
        },
        {
            "name": "num_bpe_labels",
            "type": "int",
            "optional": true,
            "default": "50257",
            "description": "The number of classes for bpe head ."
        },
        {
            "name": "num_wordpiece_labels",
            "type": "int",
            "optional": true,
            "default": "30522",
            "description": "The number of classes for wordpiece head ."
        },
        {
            "name": "hidden_size",
            "type": "int",
            "optional": true,
            "default": "768",
            "description": "The embedding dimension."
        },
        {
            "name": "num_hidden_layers",
            "type": "int",
            "optional": true,
            "default": "12",
            "description": "Number of hidden layers in the Transformer encoder."
        },
        {
            "name": "num_attention_heads",
            "type": "int",
            "optional": true,
            "default": "12",
            "description": "Number of attention heads for each attention layer in the Transformer encoder."
        },
        {
            "name": "mlp_ratio",
            "type": "float",
            "optional": true,
            "default": "4.0",
            "description": "The ratio of mlp hidden dim to embedding dim."
        },
        {
            "name": "qkv_bias",
            "type": "bool",
            "optional": true,
            "default": "True",
            "description": "Whether to add a bias to the queries, keys and values."
        },
        {
            "name": "distilled",
            "type": "bool",
            "optional": true,
            "default": "False",
            "description": "Model includes a distillation token and head as in DeiT models."
        },
        {
            "name": "layer_norm_eps",
            "type": "float",
            "optional": true,
            "default": "1e-05",
            "description": "The epsilon used by the layer normalization layers."
        },
        {
            "name": "drop_rate",
            "type": "float",
            "optional": true,
            "default": "0.0",
            "description": "The dropout probability for all fully connected layers in the embeddings, encoder."
        },
        {
            "name": "attn_drop_rate",
            "type": "float",
            "optional": true,
            "default": "0.0",
            "description": "The dropout ratio for the attention probabilities."
        },
        {
            "name": "drop_path_rate",
            "type": "float",
            "optional": true,
            "default": "0.0",
            "description": "The stochastic depth rate."
        },
        {
            "name": "output_a3_attentions",
            "type": "bool",
            "optional": true,
            "default": "False",
            "description": "Whether or not the model should returns A^3 module attentions."
        },
        {
            "name": "initializer_range",
            "type": "float",
            "optional": true,
            "default": "0.02",
            "description": "The standard deviation of the truncated_normal_initializer for initializing all weight matrices."
        }
    ],
    "return": ""
}